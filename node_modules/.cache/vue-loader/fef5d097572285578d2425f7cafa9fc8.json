his.merror = \"Seo sin opción\"\r\n                }else{\r\n                    console.log(\"procesamos\")\r\n                }\r\n            }\r\n        },\r\n        obdatos()\r\n        {\r\n            this.merror = '';\r\n            //this.esid = this.$route.params.id;\r\n            //console.log(this.esid)\r\n            axios\r\n            .post(\r\n                Global.urirl + 'consulta/',\r\n                {\r\n                    opcion:2,\r\n                    id: '1'\r\n                }\r\n            )\r\n            .then(\r\n                res=>{\r\n                    console.log(res.status)\r\n                }\r\n            )\r\n        }\r\n    }\r\n}\r\n</script>"]}]}