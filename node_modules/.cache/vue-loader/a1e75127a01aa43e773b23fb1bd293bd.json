                <option disabled value=\"\">Elija una categoria</option>\r\n                        <option>general</option>\r\n                        <option>tutorial</option>\r\n                        <option>games</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"mt-2 card p-2\">\r\n                    <p><strong>SEO</strong></p>\r\n                    <select class=\"form-select\" v-model=\"seo\">\r\n                        <option disabled value=\"\">Elija una opción</option>\r\n                        <option>index-follow</option>\r\n                        <option>index-nofollow</option>\r\n                        <option>noindex-follow</option>\r\n                        <option>noindex-nofollow</option>\r\n                    </select>\r\n                </div>\r\n           </div>\r\n        </div>\r\n    </div>\r\n    </form>\r\n</template>\r\n<script>\r\nimport Menuadm from '../components/Menuadm.vue'\r\nimport Editor from '@tinymce/tinymce-vue'\r\nimport axios from 'axios'\r\nimport { Global } from '../Global' \r\n\r\nexport default {\r\n    name:'Admnart',\r\n    data(){\r\n        return{\r\n            titulo:'',\r\n            contenido:'',\r\n            estado:'',\r\n            categoria:'',\r\n            seo:'',\r\n            merror:'',\r\n            autor: ''\r\n        }\r\n    },\r\n    components:{\r\n        Menuadm,\r\n        'editor': Editor\r\n    },\r\n    methods:{\r\n        ingreso()\r\n        {\r\n            //console.log(this.titulo)\r\n            //console.log(this.contenido)\r\n            //console.log(this.estado)\r\n            //console.log(this.categoria)\r\n            //console.log(this.seo)\r\n            axios\r\n            .post(\r\n                Global.urirl + 'insertar/',\r\n                {\r\n                    opcion:1,\r\n                    titulo: this.titulo,\r\n                    contenido : this.contenido,\r\n                    estado: this.estado,\r\n                    categoria: this.categoria,\r\n                    seo: this.seo,\r\n                    autor: this.autor\r\n                }\r\n            )\r\n            .then(\r\n                res=>{\r\n                    if(res.status==200)\r\n                    {\r\n                        console.log(res.data)\r\n                    }else{\r\n                        this.merror = \"Error de Data\"\r\n                    }\r\n                }\r\n            )\r\n        }\r\n    },\r\n    mounted(){\r\n        this.autor = ''\r\n        if(localStorage.getItem('rol')!==\"administrador\")\r\n        {\r\n            this.$router.push('/login')\r\n        }else{\r\n            this.autor = localStorage.getItem('user')\r\n        }\r\n    }\r\n}\r\n</script>"]}]}