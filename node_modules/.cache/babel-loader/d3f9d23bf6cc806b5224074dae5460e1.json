,GA3BU;AA4BX,EAAA,OAAO,EAAC;AACJ,IAAA,OADI,qBAEJ;AAAA;;AACI,WAAK,MAAL,GAAc,EAAd;AACA,WAAK,EAAL,CAAQ,SAAR;;AACA,UAAG,CAAC,KAAK,EAAL,CAAQ,MAAZ,EACA;AACI,YAAG,KAAK,MAAL,IAAa,EAAhB,EAAmB;AACf,eAAK,MAAL,GAAc,qBAAd;AACH,SAFD,MAEM,IAAG,KAAK,SAAL,IAAgB,EAAnB,EAAsB;AACxB,eAAK,MAAL,GAAc,sBAAd;AACH,SAFK,MAEA,IAAG,KAAK,GAAL,IAAU,EAAb,EAAgB;AAClB,eAAK,MAAL,GAAc,gBAAd;AACH,SAFK,MAED;AACD,UAAA,KAAI,CACH,IADD,CAEI,MAAM,CAAC,KAAP,GAAe,WAFnB,EAGI;AACI,YAAA,MAAM,EAAC,CADX;AAEI,YAAA,MAAM,EAAE,KAAK,MAFjB;AAGI,YAAA,SAAQ,EAAI,KAAK,SAHrB;AAII,YAAA,MAAM,EAAE,KAAK,MAJjB;AAKI,YAAA,SAAS,EAAE,KAAK,SALpB;AAMI,YAAA,GAAG,EAAE,KAAK,GANd;AAOI,YAAA,KAAK,EAAE,KAAK;AAPhB,WAHJ,EAaC,IAbD,CAcI,UAAA,GAAG,EAAE;AACD,gBAAG,GAAG,CAAC,MAAJ,IAAY,GAAf,EACA;AACI,cAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,kBAAkB,GAAG,CAAC,IAAxC;AACH,aAHD,MAGK;AACD,cAAA,KAAI,CAAC,MAAL,GAAc,eAAd;AACJ;AACJ,WArBJ;AAuBJ;AACJ;AACJ;AAvCI,GA5BG;AAqEX,EAAA,OArEW,qBAqEF;AACL,SAAK,KAAL,GAAa,EAAb;;AACA,QAAG,YAAY,CAAC,OAAb,CAAqB,KAArB,MAA8B,eAAjC,EACA;AACI,WAAK,OAAL,CAAa,IAAb,CAAkB,QAAlB;AACH,KAHD,MAGK;AACD,WAAK,KAAL,GAAa,YAAY,CAAC,OAAb,CAAqB,MAArB,CAAb;AACJ;AACJ;AA7EW,CAAf","sourcesContent":["<template>\r\n    <Menuadm/>\r\n    <form v-on:submit.prevent=\"ingreso()\">\r\n    <div class=\"container\">\r\n        <div class=\"row\">\r\n           <div class=\"col-12\"><h2>NUEVO ARTICULO</h2></div> \r\n           <div class=\"col-12 col-md-8\">\r\n               <div class=\"mb-4\">\r\n                    <div class=\"alert alert-danger mb-2\" v-if=\"merror && merror!=''\">{{merror}}</div>\r\n                    <div v-if=\"v$.titulo.$error\" class=\"alert alert-danger text-center mb-2\">{{ v$.titulo.$errors[0].$message }}</div>\r\n                    <input \r\n                        type=\"text\"\r\n                        placeholder=\"Título\"\r\n                        class=\"form-control\"\r\n                        v-model=\"titulo\"\r\n                   />\r\n               </div>\r\n               <div>\r\n                    <editor\r\n                        api-key=\"nftneibcgkgxik91ybc1kpaau1jmp6ljbpifsv1cnssjgq9h\"\r\n                        :init=\"{\r\n                        menubar: true,\r\n                        plugins: 'lists link image emoticons media code fullscreen hr insertdatetime pagebreak preview table codesample',\r\n                        toolbar1: 'styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist | table hr insertdatetime blockquote backcolor forecolor',\r\n                        toolbar2: 'image | media emoticons | link openlink unlink pagebreak preview | code codesample fullscreen',\r\n                        language: 'es',\r\n                        height:400,\r\n                        external_filemanager_path:'/filemanager/',\r\n                        filemanager_title:'Dogdark Filemanager',\r\n                        external_plugins: { 'filemanager' : '/filemanager/plugin.min.js'}\r\n                        }\"\r\n                        v-model=\"contenido\"\r\n                    />\r\n               </div>\r\n           </div>\r\n           <div class=\"col-12 col-md-4\">\r\n                <div class=\"mb-4\">\r\n                    <input \r\n                        type=\"submit\"\r\n                        value=\"GRABAR\"\r\n                        class=\"btn btn-success\"\r\n                    />\r\n                </div>\r\n                <div class=\"card p-2\">\r\n                    <p><strong>PUBLICAR</strong></p>\r\n                    <select class=\"form-select\" v-model=\"estado\">\r\n                        <option disabled value=\"\">Opción</option>\r\n                        <option>PUBLICAR</option>\r\n                        <option>GUARDAR</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"mt-2 card p-2\">\r\n                    <p><strong>CATEGORIA</strong></p>\r\n                    <select class=\"form-select\" v-model=\"categoria\">\r\n                        <option disabled value=\"\">Elija una categoria</option>\r\n                        <option>general</option>\r\n                        <option>tutorial</option>\r\n                        <option>games</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"mt-2 card p-2\">\r\n                    <p><strong>SEO</strong></p>\r\n                    <select class=\"form-select\" v-model=\"seo\">\r\n                        <option disabled value=\"\">Elija una opción</option>\r\n                        <option>index-follow</option>\r\n                        <option>index-nofollow</option>\r\n                        <option>noindex-follow</option>\r\n                        <option>noindex-nofollow</option>\r\n                    </select>\r\n                </div>\r\n           </div>\r\n        </div>\r\n    </div>\r\n    </form>\r\n</template>\r\n<script>\r\nimport Menuadm from '../components/Menuadm.vue'\r\nimport Editor from '@tinymce/tinymce-vue'\r\nimport axios from 'axios'\r\nimport { Global } from '../Global'\r\nimport useValidate from '@vuelidate/core'\r\nimport { required, minLength } from '@vuelidate/validators'\r\n\r\nexport default {\r\n    name:'Admnart',\r\n    data(){\r\n        return{\r\n            v$: useValidate(),\r\n            titulo:'',\r\n            contenido:'',\r\n            estado:'',\r\n            categoria:'',\r\n            seo:'',\r\n            autor: '',\r\n            merror:'',\r\n            retorno:[]\r\n        }\r\n    },\r\n    components:{\r\n        Menuadm,\r\n        'editor': Editor\r\n    },\r\n    validations()\r\n    {\r\n        return{\r\n            titulo:{\r\n                required,\r\n                minLength: minLength(10)\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        ingreso()\r\n        {\r\n            this.merror = ''\r\n            this.v$.$validate()\r\n            if(!this.v$.$error)\r\n            {\r\n                if(this.estado==\"\"){\r\n                    this.merror = \"Publicar sin opción\"\r\n                }else if(this.categoria==\"\"){\r\n                    this.merror = \"Categoría sin opción\"\r\n                }else if(this.seo==\"\"){\r\n                    this.merror = \"Seo sin opción\"\r\n                }else{\r\n                    axios\r\n                    .post(\r\n                        Global.urirl + 'insertar/',\r\n                        {\r\n                            opcion:1,\r\n                            titulo: this.titulo,\r\n                            contenido : this.contenido,\r\n                            estado: this.estado,\r\n                            categoria: this.categoria,\r\n                            seo: this.seo,\r\n                            autor: this.autor\r\n                        }\r\n                    )\r\n                    .then(\r\n                        res=>{\r\n                            if(res.status==200)\r\n                            {\r\n                                this.$router.push('/adm/admeart/' + res.data)\r\n                            }else{\r\n                                this.merror = \"Error de Data\"\r\n                            }\r\n                        }\r\n                    )\r\n                }\r\n            }\r\n        }\r\n    },\r\n    mounted(){\r\n        this.autor = ''\r\n        if(localStorage.getItem('rol')!==\"administrador\")\r\n        {\r\n            this.$router.push('/login')\r\n        }else{\r\n            this.autor = localStorage.getItem('user')\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":""}]}